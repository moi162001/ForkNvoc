CLAYMORE_VERSION="latest"                   # "latest", "latest" or the version folder name.

ETHMINER_OPTS=""                                 # Ethminer optional arguments. add "--api-port -3333" if you want web info
ETHMINER_VERSION="latest"                   # "latest", "latest" or the version folder name.

DSTM_OPTS=""                                     # DSTM Miner optional arguments add "--telemetry=$IPW:42000" if you want web info
DSTM_VERSION="latest"                       # "latest", "latest" or the version folder name.

EWBF_PERCENT="0"                                 # change EWBF_PERCENT to alter donation percent for EWBF Miner   
EWBF_OPTS=""                                     # EWBF Equihash Miner optional arguments. add "--api $IPW:42000" if you want web info
EWBF_VERSION="latest"                       # "latest", "latest" or the version folder name. If choose 3.3 you should extract 3.3 in miner folder to 3.3 folder

Z_EWBF_PERCENT="0"                               # EWBF ZHASH Dev fee percent to alter donation percent   
Z_EWBF_OPTS=""                                   # EWBF ZHASH Miner optional arguments. add "--api $IPW:42000" if you want web info
Z_EWBF_VERSION="latest"                     # "latest", "latest" or the version folder name.

